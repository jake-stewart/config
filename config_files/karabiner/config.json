{
    "title": "jake's config",
    "rules": [{
        "description": "makes my fingers feel good",
        "manipulators": [
            {
                "description": "disable lock key",
                "type": "basic",
                "from": {
                    "consumer_key_code": "al_terminal_lock_or_screensaver"
                },
                "to": []
            },
            {
                "description": "disable caps lock",
                "type": "basic",
                "from": {
                    "key_code": "caps_lock"
                },
                "to": []
            },
            {
                "description": "right cmd as escape",
                "type": "basic",
                "from": {
                    "key_code": "right_command",
                    "modifiers": {
                        "optional": [
                            "left_control",
                            "left_command",
                            "left_option",
                            "left_shift",
                            "right_shift"
                        ]
                    }
                },
                "to": [{
                    "key_code": "escape"
                }]
            },
            {
                "description": "cmd as ctrl",
                "type": "basic",
                "from": {
                    "key_code": "left_command"
                },
                "to": [{
                    "key_code": "left_control"
                }]
            },
            {
                "description": "option as cmd",
                "conditions": [{
                    "type": "frontmost_application_unless",
                    "bundle_identifiers": [
                        "^com\\.microsoft\\.rdc\\.macos$"
                    ]
                }],
                "type": "basic",
                "from": {
                    "key_code": "left_option"
                },
                "to": [{
                    "key_code": "left_command"
                }]
            },
            {
                "description": "ctrl as option",
                "conditions": [{
                    "type": "frontmost_application_unless",
                    "bundle_identifiers": [
                        "^com\\.microsoft\\.rdc\\.macos$"
                    ]
                }],
                "type": "basic",
                "from": {
                    "key_code": "left_control"
                },
                "to": [{
                    "key_code": "left_option"
                }]
            },
            {
                "description": "ctrl as cmd",
                "conditions": [{
                    "type": "frontmost_application_if",
                    "bundle_identifiers": [
                        "^com\\.microsoft\\.rdc\\.macos$"
                    ]
                }],
                "type": "basic",
                "from": {
                    "key_code": "left_control"
                },
                "to": [{
                    "key_code": "left_command"
                }]
            },
            {
                "description": "cmd + j as enter globally",
                "type": "basic",
                "from": {
                    "key_code": "j",
                    "modifiers": {
                        "mandatory": [
                            "left_control"
                        ]
                    }
                },
                "to": [{
                    "key_code": "return_or_enter"
                }]
            },
            {
                "description": "macOS binds cmd+h to hide the window. Replace with option+h",
                "type": "basic",
                "from": {
                    "key_code": "h",
                    "modifiers": {
                        "mandatory": [
                            "left_command"
                        ]
                    }
                },
                "to": [{
                    "key_code": "h",
                    "modifiers": [
                        "option"
                    ]
                }]
            },
            {
                "description": "macOS binds cmd+m to minimize the window. Replace with option+m",
                "type": "basic",
                "from": {
                    "key_code": "m",
                    "modifiers": {
                        "mandatory": [
                            "left_command"
                        ]
                    }
                },
                "to": [{
                    "key_code": "m",
                    "modifiers": [
                        "option"
                    ]
                }]
            },
            {
                "description": "bind cmd+shift+j to option+shift+j",
                "type": "basic",
                "from": {
                    "key_code": "j",
                    "modifiers": {
                        "mandatory": [
                            "left_command",
                            "left_shift"
                        ]
                    }
                },
                "to": [{
                    "key_code": "j",
                    "modifiers": [
                        "option",
                        "shift"
                    ]
                }]
            },
            {
                "description": "bind cmd+shift+k to option+shift+k",
                "type": "basic",
                "from": {
                    "key_code": "k",
                    "modifiers": {
                        "mandatory": [
                            "left_command",
                            "left_shift"
                        ]
                    }
                },
                "to": [{
                    "key_code": "k",
                    "modifiers": [
                        "option",
                        "shift"
                    ]
                }]
            },
            {
                "type": "basic",
                "conditions": [{
                    "type": "frontmost_application_unless",
                    "bundle_identifiers": [
                        "^com\\.microsoft\\.rdc\\.macos$"
                    ]
                }],
                "from": {
                    "key_code": "k",
                    "modifiers": {
                        "mandatory": [
                            "left_command"
                        ]
                    }
                },
                "to": [{
                    "key_code": "tab",
                    "modifiers": [
                        "left_control"
                    ]
                }]
            },
            {
                "type": "basic",
                "conditions": [{
                    "type": "frontmost_application_unless",
                    "bundle_identifiers": [
                        "^com\\.microsoft\\.rdc\\.macos$"
                    ]
                }],
                "from": {
                    "key_code": "j",
                    "modifiers": {
                        "mandatory": [
                            "left_command"
                        ]
                    }
                },
                "to": [{
                    "key_code": "tab",
                    "modifiers": [
                        "left_control",
                        "left_shift"
                    ]
                }]
            },
            {
                "type": "basic",
                "conditions": [{
                    "type": "frontmost_application_if",
                    "bundle_identifiers": [
                        "^com\\.microsoft\\.rdc\\.macos$"
                    ]
                }],
                "from": {
                    "key_code": "k",
                    "modifiers": {
                        "mandatory": [
                            "left_option"
                        ]
                    }
                },
                "to": [{
                    "key_code": "tab",
                    "modifiers": [
                        "left_control"
                    ]
                }]
            },
            {
                "type": "basic",
                "conditions": [{
                    "type": "frontmost_application_if",
                    "bundle_identifiers": [
                        "^com\\.microsoft\\.rdc\\.macos$"
                    ]
                }],
                "from": {
                    "key_code": "j",
                    "modifiers": {
                        "mandatory": [
                            "left_option"
                        ]
                    }
                },
                "to": [{
                    "key_code": "tab",
                    "modifiers": [
                        "left_control",
                        "left_shift"
                    ]
                }]
            },
            {
                "type": "basic",
                "conditions": [{
                    "type": "frontmost_application_if",
                    "bundle_identifiers": [
                        "^com\\.microsoft\\.rdc\\.macos$"
                    ]
                }],
                "from": {
                    "key_code": "space",
                    "modifiers": {
                        "mandatory": [
                            "left_option"
                        ]
                    }
                },
                "to": [{
                    "key_code": "space",
                    "modifiers": [
                        "left_command"
                    ]
                }]
            }
        ]
    }]
}
